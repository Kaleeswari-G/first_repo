pipeline
{
   agent any
   parameters {
	  choice(name: 'ENVIRONMENT', choices: ['DEV','STAGE'], description: 'Select on which environment you want to deploy the application')
	}
   stages
   {
     stage('Hello')
     {
        steps
        {     
           echo "Hello"
           echo "${params.ENVIRONMENT}"
        }
     }
   }
}
